<Window x:Class="FundTool.Directas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FundTool"
        mc:Ignorable="d"
        Title="Directas" Height="450" Width="800">
    <Grid Background="#D3D3D3">
        <DockPanel HorizontalAlignment="Left" Height="130" LastChildFill="False" Margin="10,10,0,0" VerticalAlignment="Top" Width="263" Background="AntiqueWhite">
            <Label Content="Materiales"/>
        </DockPanel>
        <Label Content="Resistencia del Concreto:" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" Width="143"/>
        <TextBox x:Name="ResistenciaConcreto" HorizontalAlignment="Left" Height="23" Margin="153,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="45" PreviewTextInput="NumericOnly" TextChanged="ResistenciaDelConcreto"/>
        <Label Content="kg/cm²" HorizontalAlignment="Left" Margin="203,38,0,0" VerticalAlignment="Top"/>
        <Label Content="Resistencia del Acero:&#xD;&#xA;" HorizontalAlignment="Left" Margin="10,69,0,0" VerticalAlignment="Top" Width="133" Height="27"/>
        <TextBox x:Name="ResistenciaAcero" HorizontalAlignment="Left" Height="23" Margin="153,73,0,0" TextWrapping="Wrap" Text="&#xD;&#xA;" VerticalAlignment="Top" Width="45" PreviewTextInput="NumericOnly" TextChanged="ResistenciaDelAcero"/>
        <Label Content="kg/cm²" HorizontalAlignment="Left" Margin="203,70,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Aceptarm" Content="Aceptar" HorizontalAlignment="Left" Margin="145,102,0,0" VerticalAlignment="Top" Width="53" Click="AceptarMateriales"/>
        <Button x:Name="Cancelarm" Content="Cancelar" HorizontalAlignment="Left" Margin="218,102,0,0" VerticalAlignment="Top" Width="55" Click="CancelarMateriales"/>
    </Grid>
</Window>
